            //IFRAME tab_banco contenidos por categoria (noticias/publicaciones/agenda semana/etc)
            $('#pill1, #pill2, #pill3, #pill4, #pill5').on('load', function() {
                // Ahora estamos dentro del controlador de carga del iframe

                const iframeContents = $(this).contents();

                iframeContents.find('a').on('click', function(event) {
                    // Ahora estamos dentro del controlador de clics del enlace

                    /*event.preventDefault();*/

                    const iframeTag = $(this);
                    const timestampOrigen = Date.now();
                    const fechaLegible = new Date(timestampOrigen).toLocaleString();

                    var dataPush = {
                        event: 'go to',
                        'event_params':  {
                            'event_action': 'open',
                            'event_category': 'link',
                            'event_label': $.trim(iframeTag.text()),
                            'event_position': 'center',
                            'event_structure_position': 'body',
                            'page_name': document.title
                        },
                        titulo: $.trim(iframeTag.text()),
                        urlOrigen: window.location.href,
                        urlDestino: iframeTag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };

                    dataLayer.push(dataPush);
                    console.log('Evento Parametro Pagina:',dataPush['event_params']['page_name'],'||','Tipo Evento:',dataPush.event,'||','Titulo Pagina:', dataPush.titulo,'||','urlOrigen:', dataPush.urlOrigen,'||','urlDestino:', dataPush.urlDestino,'||','Fecha Visita:', fechaLegible);

                });
            });

