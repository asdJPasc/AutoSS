<script type="text/javascript">
	function blockCharacters(e){
		 var key = e.keyCode || e.which,
		 tecla = String.fromCharCode(key).toLowerCase(),
		 caracteres="\\",
		 especiales = [8, 37, 39, 46],
		 tecla_especial = false;
		 
		 
		 for(var i in especiales){
			 if(key ==  especiales[i]){
				 tecla_especial = true;
				 break;
			 }
		 }
		 
		 if(caracteres.indexOf(tecla) != -1 && !tecla_especial){
			 return false;
		 }
		 
		 
	}
	function redirect(url){
		window.open(url, '_blank');
	}
	
	function link_ir(url, lang) {
		var idioma = "&Idioma="+lang.toUpperCase();
		var link = url+document.getElementById('search-form-text1').value+idioma;
		window.open(link, '_blank');
	}
	
	var busquedaUrl1 = '/buscador';
	var busquedaExacta1 = $('#busqueda-exacta1');
	
	$(document).ready(function() {
		selecExacta1("false");
	});
	
	
	function selecExacta1(valor){
		if(valor == "true"){
			busquedaExacta1.val("false");
		}else if(valor == "false"){
			busquedaExacta1.val("true");
		}
	}
	
	$('#search-form1').on('click',function(){
		var newUrl1 = busquedaUrl1+"?";
		var searchText = $("#search-form-text1").val().trim();
	
		if(searchText != ""){
			if(busquedaExacta1.val() == "true"){

				var urlString = decodeURIComponent(window.location.href);
				var url = new URL(urlString);
				var param_Categoria = url.searchParams.get("categoria");

				newUrl1 = newUrl1+"keyword="+searchText;
				newUrl1 = newUrl1+"&busquedaExacta=true";

				if(param_Categoria == null){
					console.log("No contiene categoria");
				}else if(param_Categoria === ""){
					newUrl1 = newUrl1+"&categoria=";
				}else{
					newUrl1 = newUrl1+"&categoria="+param_Categoria;
				}

				$('#search-form1').attr('href',newUrl1);
			}else{

				var urlString1 = decodeURIComponent(window.location.href);
				var url1 = new URL(urlString1);
				var param_Categoria = url1.searchParams.get("categoria");

				newUrl1 = newUrl1+"keyword="+searchText;

				if(param_Categoria == null){
					console.log("No contiene categoria");
				}else if(param_Categoria === ""){
					newUrl1 = newUrl1+"&categoria=";
				}else{
					newUrl1 = newUrl1+"&categoria="+param_Categoria;
				}

				$('#search-form1').attr('href',newUrl1);
			}
		}else{
			console.log("Input vacio");
		}
	});
	
	$('#search-form-text1').keypress(function(event){
		if (event.keyCode === 13) {
			event.preventDefault();
			
			
			var newUrl = busquedaUrl1+"?";
			var searchText = $("#search-form-text1").val().trim();
			
			if(searchText != ""){
				if(busquedaExacta1.val() == "true"){
					
					var urlString = decodeURIComponent(window.location.href);
					var url = new URL(urlString);
					var param_Categoria = url.searchParams.get("categoria");

					newUrl = newUrl+"keyword="+searchText;
					newUrl = newUrl+"&busquedaExacta=true";

					if(param_Categoria == null){
						console.log("No contiene categoria");
					}else if(param_Categoria === ""){
						newUrl = newUrl+"&categoria=";
					}else{
						newUrl = newUrl+"&categoria="+param_Categoria;
					}

				}else{
					
					var urlString = decodeURIComponent(window.location.href);
					var url = new URL(urlString);
					var param_Categoria = url.searchParams.get("categoria");

					newUrl = newUrl+"keyword="+searchText;

					if(param_Categoria == null){
						console.log("No contiene categoria");
					}else if(param_Categoria === ""){
						newUrl = newUrl+"&categoria=";
					}else{
						newUrl = newUrl+"&categoria="+param_Categoria;
					}
					
				}
				
				window.location.replace(newUrl);
			}else{
				console.log("Input vacio");
			}
	    }
	});
</script>

	
	
					
				
			
		
	
	


	</div>

			</div>
		
	</div>
</section>
	

		
		







	</div>










































	

