<div class="d-flex justify-content-center mb-4">
	
		
			<input class="search with-search-exactly"  id="search-form-text1" onkeypress='return blockCharacters(event)' placeholder="Buscar" type="search">
		
	
  	
   	<div class="box-busqueda-exacta">
	   	
			
				<input class="styled-checkbox small-check" id="busqueda-exacta1" type="checkbox" onclick="selecExacta1(this.value);" value="true" checked>
			
			
		
        <label for="busqueda-exacta1"></label>
        <label for="busqueda-exacta1" class="box-busqueda-exacta-text">B&uacute;squeda exacta</label>
