<script>
    $(function(){
        setTimeout(function() {
            console.log('GA4 v2');

            //Eliminar role=menuitem del cambio de idioma
            $('#idioma').find('[target="_self"]').removeAttr('role');

            /* obtener el breadcrumb para generar titulos personificados para los eventos */
            var elementos = $('[aria-label="Ruta de navegación"] , [aria-label="Breadcrumb"]').find('li');
            var cadena = "";
            elementos.each(function(index) {
                var contenido = $(this).text().trim().replace(/\s+/g, ' ');
                if (index !== elementos.length - 1) {
                    contenido += '/';
                }
                cadena += contenido;
            });
            /*console.log(cadena);*/

            /*------------- Idioma ------------- */

            $('header #actionIdioma').on('click',function(){
                var tag = $(this);
                window.dataLayer = window.dataLayer || [];
                var timestampOrigen = $.now();
                var fecha = new Date(timestampOrigen);
                var fechaLegible = fecha.toLocaleString();
                var urlOrigen = $(location).attr('href');

                //IF English/Spanish
                var dataPush = {
                    event: 'interaction',
                    'event_params':  {
                        'event_action': 'click',
                        'event_category': 'button',
                        'event_label': 'Idioma sitio: '+tag.text(),
                        'event_position': 'top',
                        'event_structure_position': 'header',
                        'page_name': document.title
                    },
                    titulo: 'Idioma sitio: '+tag.text(),
                    urlOrigen: urlOrigen,
                    urlDestino: tag.attr('href'),
                    timestamp: timestampOrigen,
                    fechaLegible: fechaLegible
                };

                dataLayer.push(dataPush);
                console.log('Evento Parametro Pagina:',dataPush['event_params']['page_name'],'||','Tipo Evento:',dataPush.event,'||','Titulo Pagina:', dataPush.titulo,'||','urlOrigen:', dataPush.urlOrigen,'||','urlDestino:', dataPush.urlDestino,'||','Fecha Visita:', fechaLegible);

            });

            /*------------- Header ------------- */
            //TAG Header Footer a
            $('header a, footer a').on('click',function(){
                var tag = $(this);
                window.dataLayer = window.dataLayer || [];
                var timestampOrigen = $.now();
                var fecha = new Date(timestampOrigen);
                var fechaLegible = fecha.toLocaleString();
                var urlOrigen = $(location).attr('href');

                if( tag.attr('role') == 'menuitem' ){
                    //IF Menu principal MePri
                    var dataPush = {
                        event: 'go to',
                        'event_params':  {
                            'event_action': 'click',
                            'event_category': 'button',
                            'event_label': 'MePri/'+tag.attr('title'),
                            'event_position': 'top',
                            'event_structure_position': 'header',
                            'page_name': document.title
                        },
                        titulo: 'MePri/'+tag.attr('title'),
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                }else if( tag.attr('role') == 'footermenuitem' ){
                    //IF FOOT
                    var dataPush = {
                        event: 'go to',
                        'event_params':  {
                            'event_action': 'click',
                            'event_category': 'button',
                            'event_label': 'FOOT/'+tag.attr('title'),
                            'event_position': 'bottom',
                            'event_structure_position': 'footer',
                            'page_name': document.title
                        },
                        titulo: 'FOOT/'+tag.attr('title'),
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                }else if( tag.hasClass('accebility-bar-contraste') ){
                    //IF Modo Oscuro/Light
                    var dataPush = {
                        event: 'interaction',
                        'event_params':  {
                            'event_action': 'click',
                            'event_category': 'tooltip',
                            'event_label': 'day/night',
                            'event_position': 'top',
                            'event_structure_position': 'header',
                            'page_name': document.title
                        },
                        titulo: 'day/night',
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                }else if( tag.hasClass('accebility-bar-aumentarFont') ){
                    //IF Aumentar Font
                    var dataPush = {
                        event: 'interaction',
                        'event_params':  {
                            'event_action': 'click',
                            'event_category': 'tooltip',
                            'event_label': 'Aumentar',
                            'event_position': 'top',
                            'event_structure_position': 'header',
                            'page_name': document.title
                        },
                        titulo: 'Aumentar',
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                }else if( tag.hasClass('accebility-bar-disminuirFont') ){
                    //IF Disminuir Font
                    var dataPush = {
                        event: 'interaction',
                        'event_params':  {
                            'event_action': 'click',
                            'event_category': 'tooltip',
                            'event_label': 'Disminuir',
                            'event_position': 'top',
                            'event_structure_position': 'header',
                            'page_name': document.title
                        },
                        titulo: 'Disminuir',
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                }else if(tag.attr('id') == 'search-form'){
                    //buscador header
                    let textBusquedaHeader = tag.siblings().closest('.searchbox').find('#search-form-text').val();
                    var dataPush = {
                        event: 'interaction',
                        'event_params': {
                            'event_action': 'search',
                            'event_category': 'tooltip',
                            'event_label': 'Buscador/' + $.trim(textBusquedaHeader),
                            'event_position': 'center',
                            'event_structure_position': 'body',
                            'page_name': document.title
                        },
                        titulo: 'Buscador/' + $.trim(textBusquedaHeader),
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };
                } else {
                    return;
                }

                dataLayer.push(dataPush);
                console.log('Evento Parametro Pagina:',dataPush['event_params']['page_name'],'||','Tipo Evento:',dataPush.event,'||','Titulo Pagina:', dataPush.titulo,'||','urlOrigen:', dataPush.urlOrigen,'||','urlDestino:', dataPush.urlDestino,'||','Fecha Visita:', fechaLegible);
            });

            /*------------- body ------------- */
// Función onclick para el botón de flecha izquierda del banner de cabecera
            function handleCarouselControlPrevClick(tag, urlOrigen, timestampOrigen, fechaLegible) {
                var dataPush = {
                    event: 'interaction',
                    'event_params': {
                        'event_action': 'swipetoleft',
                        'event_category': 'tooltip',
                        'event_label': 'arrowbanner',
                        'event_position': 'top',
                        'event_structure_position': 'carousel',
                        'page_name': document.title
                    },
                    titulo: 'BCaB-swipetoleft',
                    urlOrigen: urlOrigen,
                    urlDestino: tag.attr('href'),
                    timestamp: timestampOrigen,
                    fechaLegible: fechaLegible
                };
                dataLayer.push(dataPush);
                console.log('Evento Parametro Pagina:', dataPush['event_params']['page_name'], '||', 'Tipo Evento:', dataPush.event, '||', 'Titulo Pagina:', dataPush.titulo, '||', 'urlOrigen:', dataPush.urlOrigen, '||', 'urlDestino:', dataPush.urlDestino, '||', 'Fecha Visita:', fechaLegible);
            }

// Función onclick para el botón de flecha derecha del banner de cabecera
            function handleCarouselControlNextClick(tag, urlOrigen, timestampOrigen, fechaLegible) {
                var dataPush = {
                    event: 'interaction',
                    'event_params': {
                        'event_action': 'swipetoright',
                        'event_category': 'tooltip',
                        'event_label': 'arrowbanner',
                        'event_position': 'top',
                        'event_structure_position': 'carousel',
                        'page_name': document.title
                    },
                    titulo: 'BCaB-swipetoright',
                    urlOrigen: urlOrigen,
                    urlDestino: tag.attr('href'),
                    timestamp: timestampOrigen,
                    fechaLegible: fechaLegible
                };
                dataLayer.push(dataPush);
                console.log('Evento Parametro Pagina:', dataPush['event_params']['page_name'], '||', 'Tipo Evento:', dataPush.event, '||', 'Titulo Pagina:', dataPush.titulo, '||', 'urlOrigen:', dataPush.urlOrigen, '||', 'urlDestino:', dataPush.urlDestino, '||', 'Fecha Visita:', fechaLegible);
            }

// Función onclick para los banners individuales dentro del banner de cabecera/bottom
            function handleBannerClick(tag, urlOrigen, timestampOrigen, fechaLegible) {
                var banners = document.getElementsByClassName("carousel-caption");
                var posisionBanner = -1;
                var getDatoBanner = tag.closest('.carousel-item').attr('data-orderbanner')
                var numeracionBanner = parseInt(getDatoBanner, 10) + 1;

                // Verificar si es el banner superior o inferior
                for (var i = 0; i < banners.length; i++) {
                    if (banners[i].contains(tag[0])) {
                        posisionBanner = i;
                        break;
                    }
                }

                if (posisionBanner !== -1) {
                    if (posisionBanner >= 0 && posisionBanner < banners.length) {
                        // Es el banner superior
                        var dataPush = {
                            event: 'go to',
                            'event_params': {
                                'event_action': 'open',
                                'event_category': 'link',
                                'event_label': $.trim(tag.children().text()),
                                'event_position': 'top',
                                'event_structure_position': 'carousel',
                                'page_name': document.title,
                                'action_name': 'click banner superior',
                                'event_order': numeracionBanner.toString()
                            },
                            titulo:  tag.attr('title'),
                            urlOrigen: urlOrigen,
                            urlDestino: tag.attr('href'),
                            timestamp: timestampOrigen,
                            fechaLegible: fechaLegible
                        };

                        dataLayer.push(dataPush);
                        console.log('Evento Parametro Pagina:', dataPush['event_params']['page_name'], '||', 'Tipo Evento:', dataPush.event, '||', 'Titulo Pagina:', dataPush.titulo, '||', 'urlOrigen:', dataPush.urlOrigen, '||', 'urlDestino:', dataPush.urlDestino, '||', 'Fecha Visita:', fechaLegible);
                    }
                } else if (tag.hasClass('new-enlace')) {
                    // Es el banner inferior
                    var bannersInferiores = document.getElementsByClassName("new-banner-slider");
                    var posisionBannerInferior = Array.prototype.indexOf.call(bannersInferiores, tag.closest('.new-banner-slider'));

                    var dataPush = {
                        event: 'go to',
                        'event_params': {
                            'event_action': 'open',
                            'event_category': 'link',
                            'event_label': $.trim(tag.text().replace(/\s+/g, ' ')),
                            'event_position': 'bottom',
                            'event_structure_position': 'carousel',
                            'page_name': document.title,
                            'action_name': 'click banner inferior',
                            'event_order': numeracionBanner.toString()
                        },
                        titulo: $.trim(tag.text().replace(/\s+/g, ' ')),
                        urlOrigen: urlOrigen,
                        urlDestino: tag.attr('href'),
                        timestamp: timestampOrigen,
                        fechaLegible: fechaLegible
                    };

                    dataLayer.push(dataPush);
                    console.log('Evento Parametro Pagina:', dataPush['event_params']['page_name'], '||', 'Tipo Evento:', dataPush.event, '||', 'Titulo Pagina:', dataPush.titulo, '||', 'urlOrigen:', dataPush.urlOrigen, '||', 'urlDestino:', dataPush.urlDestino, '||', 'Fecha Visita:', fechaLegible);
                }
            }




