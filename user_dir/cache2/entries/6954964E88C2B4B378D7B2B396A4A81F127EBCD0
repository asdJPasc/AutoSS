    <script>
		document.addEventListener('DOMContentLoaded', function() {
			f_get_redsocial();
		
		});

	function f_get_redsocial()
		{
			//var vurl = "https://qa.editoraperu.com.pe/webdop-api/api/consulta/redSocialGpo";
			var vurl = "https://servicios.editoraperu.com.pe/webdop-api/api/consulta/redSocialGpo";
			
			fetch(vurl)
			.then(response => response.json())
			.then(data => {

				const urlRedSocial = document.getElementById('urlRedSocial');
				
				if (data.status == "S"){
					urlRedSocial.setAttribute('href', data.data.url);
				}
				else{
					console.log(data.message);
					urlRedSocial.setAttribute('href', "#");
				}
				
							
			})
			.catch(error => {
				// Manejar errores en caso de que la solicitud falle
				console.error('Error al obtener datos:', error);
			});
		
		}


        function downloadURI(uri, name) {
            var link = document.createElement("a");
            link.download = name;
            link.href = uri;
            link.click();
        }

        $(function () {
            $('a.show-option').click(function (evt) {
                evt.preventDefault();
                $(this).siblings('.options').slideToggle();
            });
        })
    </script>

