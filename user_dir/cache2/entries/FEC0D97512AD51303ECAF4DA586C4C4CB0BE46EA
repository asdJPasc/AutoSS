//window.addEventListener("orientationchange", function() {
window.onresize = function(){
setTimeout(function(){
//alert('resize');
 
 //alert($(".boxedContent:visible:first").width() + ":" + $(".boxedContent:visible:first").height());
var modifier = 0;
//console.log(screen.orientation);
 if ($("#bm-nav-icon:visible").length > 0) {
 //alert(window.orientation +":" + window.innerWidth + ":" + window.innerHeight);
	if(window.orientation===90 || window.orientation===-90 || window.innerWidth > window.innerHeight)//landscape
	{
		modifier = 90;
	}
	else// if(screen.orientation.angle===0)//portrait
	{
		modifier = 40;
	}
	
     //anytime orienation changes run below
    //alert("the orientation of the device is now " + screen.orientation.angle);

	//resize scrolltables
	$(".scrolltable").each(function(si,s){
	var acc = 0;
	var tbl = $(s);
	var divW = $(s).closest("div.boxedContent").getHiddenDimensions(true).innerWidth;
	if(tbl.parents("div.ui-accordion-content").length > 0){
		divW = tbl.closest("div[webpartid]").getHiddenDimensions(true).innerWidth;
		if(tbl.parents("div.ui-accordion-content:hidden").length>0){
		acc = 1;
		tbl.closest("div[webpartid]").show();
		}
		} 
	//console.log(divW);
	//console.log(modifier);
	var maxW = (divW-modifier)/2;
	//alert(maxW);
	//console.log(maxW);
	//$(".scrolltable-fixed table").find("td,th").css({'max-width':maxW+'px'});
	$(".scrolltable-fixed,.scrolltable-scroll").css({'width':maxW+'px'});
	$(".scrolltable-fixed table").find("td,th").css({'max-width':maxW+'px'});
		//get width of data table
	    var w =  tbl.find(".scrolltable-scroll-inner table").getHiddenDimensions(true).innerWidth;// + tbl.find(".scrolltable-fixed table").width();
	    //set the width of the inner div which contains the data table
		tbl.find(".scrolltable-scroll-inner").css({'width' : w+'px'});
		//console.log(divW);
		w = tbl.find(".scrolltable-fixed table:first").getHiddenDimensions(true).innerWidth;
		//console.log('fixed table width: ' + w);
		//console.log('containing div width:' + divW);
		w = divW - w - 20;// - 42;
		
		//console.log($(tbl).prop('id') + ":" + w);
		//tbl.find(".scrolltable-scroll").css({'width':w+'px'});
		
		if(w > tbl.find(".scrolltable-scroll-inner").getHiddenDimensions(true).innerWidth ){
		//console.log('greater');
			tbl.find(".scrolltable-scroll-inner, .scrolltable-scroll-inner table").css({'width':'100%'});
		}//end width check
	syncRowHeights(tbl);
	if(acc===1)
		tbl.closest("div[webpartid]").hide();
		
	});//end each scrolltable
	
	
 
}//end mobile nav check
    },500);
//}); //end orientation change event
}


 
 ␀␀␀␀␀␀␀␀W␀x␀␀␀␀␀␀20240222211730-8lW␀hC␀␀␀https://www.ginniemae.gov/Style%20Library/GNMA%20Scripts/content.js␀␀␀␀␀␀␀␀//Optional parameter includeMargin is used when calculating outer dimensions  
(function ($) { 
    $.fn.getHiddenDimensions = function (includeMargin) {
        var $item = this,
        props = { position: 'absolute', visibility: 'hidden', display: 'block' },
        dim = { width: 0, height: 0, innerWidth: 0, innerHeight: 0, outerWidth: 0, outerHeight: 0 },
        $hiddenParents = $item.parents().addBack().not(':visible'),
        includeMargin = (includeMargin == null) ? false : includeMargin;

        var oldProps = [];
        $hiddenParents.each(function () {
            var old = {};

            for (var name in props) {
                old[name] = this.style[name];
                this.style[name] = props[name];
            }

            oldProps.push(old);
        });

        dim.width = $item.width();
        dim.outerWidth = $item.outerWidth(includeMargin);
        dim.innerWidth = $item.innerWidth();
        dim.height = $item.height();
        dim.innerHeight = $item.innerHeight();
        dim.outerHeight = $item.outerHeight(includeMargin);

        $hiddenParents.each(function (i) {
            var old = oldProps[i];
            for (var name in props) {
                this.style[name] = old[name];
            }
        });

        return dim;
    }
}(jQuery));	 


var inDesignMode = 0;
function GenerateAccordions(){
//alert(inDesignMode);
inDesignMode = typeof document.forms[MSOWebPartPageFormName] != 'undefined' ?  document.forms[MSOWebPartPageFormName].MSOLayout_InDesignMode.value : SP.Ribbon.PageState.Handlers.isInEditMode();

if (inDesignMode === "1"){
return false;
}

//$("head").append("<link rel=\"stylesheet\" type=\"text/css\" href=\"/Style%20Library/GNMA%20CSS/content.css\" />");

 
$("div.startAccordion").each(function(i,acc){
 
//var webparts = $(item).closest("tr td[id*=MSOZoneCell_WebPartWPQ]").closest("tbody>tr").nextAll();//.closest("tbody").find(">tr");
var startIndex = $(acc).closest("div[id*=MSOZoneCell_WebPartWPQ]").closest("div").index()-1;
var webparts = startIndex == -1 ? $(acc).closest("div.ms-webpart-zone").find(">div") : $(acc).closest("div.ms-webpart-zone").find(">div:gt("+startIndex+")");
webparts.each(function(wi,webpart){
var title = $(webpart).find(".ms-webpart-titleText span:first").text();
title = title.replace('(Hidden)','');

//console.log(title);
if(title.indexOf("Start Accordion")>-1){
//console.log("creating accordion");
$(webpart).replaceWith("<div class='contentAccordion' style='display:none;' id=\"accordionContainer_" + i.toString() + "\"></div>"); //find("div:first")
}
else if(title.indexOf("End Accordion")>-1){
$(webpart).remove();
return false;//break out of this one, accordion finished
}
else{
 
//console.log("appending section");
if(title.indexOf("{") > -1){

const regex = /{([^}]*)}/;
const match = regex.exec(title);
const textBetweenBrackets = match ? match[1] : null;
title = title.replace("{"+textBetweenBrackets+"}",'');
title = "<span style='display: block;font-weight: bold !important;color: black;font-size: 14px !important;'>" + textBetweenBrackets + "</span>" + title;
}
$("div[id='accordionContainer_" + i.toString() + "']").append("<h3><span>" + title + "</span></h3>").append($(webpart).find("div[webpartid]:first"));
$(webpart).remove();
}
});
 
});

$("div[id*='accordionContainer_']").accordion({heightStyle: "content", autoHeight: false, collapsible:true, active:false,  clearStyle: true, icons: {"header": "ui-icon-circle-plus","headerSelected": "ui-icon-circle-minus"}}).show();

var hash = decodeURIComponent(window.location.hash.replace(/</g, "&lt;").replace(/>/g, "&gt;"));
hash = hash.replace('#','');
var scrollToEl = $(".shareContentDiv").nextAll("div.centeredContent, div.expandedContent").find("*[id='"+hash+"'],*[name='"+hash+"'],span:contains('"+hash+"')");
if(hash!='' && hash != null && $("h3.ui-accordion-header").length > 0){
linkToAccordion(hash);
}
else if(hash!='' && hash != null && scrollToEl.length > 0){
$("html").animate({scrollTop: scrollToEl.first().offset().top - 35}, 1000);
}

 
}

function linkToAccordion(hash){
var firstMatchFound = 0;
var hashList = hash.split(';');
var firstItem = null;
$(hashList).each(function(hi,hashitem){
var item = $(".shareContentDiv").nextAll("div.centeredContent, div.expandedContent").find("h3.ui-accordion-header span:contains('"+hashitem+"')").first().parent(); //REMOVED FROM FIND *[id='"+hashitem+"'],*[name='"+hashitem+"'],
	
var a = item.closest("div[id*='accordionContainer_']");
if(a.length > 0){
	var i = $("h3.ui-accordion-header").index(item);		
	if(i > -1 && firstMatchFound == 0){	
		firstItem = item;
		firstMatchFound = 1;
		a.accordion({'active' : i});
		//$("html").animate({scrollTop: a.parent().offset().top + 200}, 1000);
		
		 a.accordion({
	  beforeActivate: function( event, ui ) {
	  $(".accoddball").next('div').hide();
	  $(".accoddball").removeClass('ui-accordion-header-active');
	  $(".accoddball").removeClass('ui-state-active');
	  $(".accoddball").removeClass('accoddball');

	  },
	  activate: function(event, ui) {
        $("html").animate({scrollTop: (firstItem.offset().top - 80)}, 1000);
      }
	  
	  });
		

	}
	else if(i > -1){
	
	var el = a.find(".ui-accordion-content").eq(i);
	  el.show();
	  item.addClass('ui-accordion-header-active ui-state-active accoddball');
	 
	 }
	}
	
});

return false;
}
