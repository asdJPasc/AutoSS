$("div.filter-table").each(function(fti,ft){
$(ft).hide();

var ftc = $("<div class='filter-table-cont' id='filter-table-cont-"+fti+"'></div>").insertAfter(ft);
var pos = 2;
$(ft).find("div.filter-tableRow:first div.filter-tableCell").each(function(i,r){


var block;


var ctrl = $(ft).find("div.filter-tableRow:eq(1) div.filter-tableCell:eq("+i+")").find(".gmFilterBlock:first, input:first").detach();
if(ctrl!=null){
block = $("<div class='filterBlock'><div class='filterBlockLabel'>"+$(r).text()+"</div><div class='filterBlockControl filter-tableCell'></div></div>").appendTo(ftc);

if(!(pos % 2 === 0))
	block.css({'float':'right'});

block.find("div.filterBlockControl").append(ctrl);
 
if(block.find("div.filterBlockControl input[type=text]").length > 0 ){block.css({'width':'90%'});block.prev().css({'float':'right'});pos=pos+2;}else{pos=pos+1;}//search box
if(block.find("div.filterBlockControl input[value='Search'], div.filterBlockControl input[value='Clear']").length > 0 && block.prev().css('float')==='left' && block.prev().css('width') !='100%'){block.css({'float':'left', 'width':'100%'});block.prev().css({'width':'100%'});pos=pos+1;}
}
});//end each cell
//$(ftc).find("input[type='button']:last").closest(".filterBlock").css({'float':'right'});
//$(ftc).closest('.boxedContent').find("table:first").contents().not("h1").wrap("<div class='card-wrapper card-2'></div>");
});//end each filter table
 

