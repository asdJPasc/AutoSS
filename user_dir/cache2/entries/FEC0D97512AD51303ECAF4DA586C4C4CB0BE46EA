}

*/



var w = 0;
var dataTbl = 0;
//get datatable width
dataTbl = tbl.find(".scrolltable-scroll-inner table").getHiddenDimensions(true).width;// + tbl.find(".scrolltable-fixed table").width();
//set to inner scroll area to accomodate width
tbl.find(".scrolltable-scroll-inner").css({'width': dataTbl+'px'});

//get scrolltable scroll width from remainder of space left after fixed table within div container
var sfTbl = tbl.find(".scrolltable-fixed table:first").getHiddenDimensions(true).width;
if(divW < 1)
	divW = tbl.closest("div.boxedContent").getHiddenDimensions(true).width;
	
w = divW - sfTbl - 25;
tbl.find(".scrolltable-scroll").css({'width':w+'px'});
//console.log('scrolltable-scroll w:' + w + ' from ' + divW + " - " + sfTbl);

//console.log(divW);
//console.log($(tbl).prop('id') + ":" + w);

if(dataTbl <= w  ){
//console.log('greater');
	tbl.find(".scrolltable-scroll-inner, .scrolltable-scroll-inner table").css({'width':'100%'});
    }
    
//hand fixed / scroll tables out of alignment
while(tbl.find(".scrolltable-scroll").position().top > tbl.find(".scrolltable-fixed").position().top ){
	tbl.find(".scrolltable-scroll").css({'width': (tbl.find(".scrolltable-scroll").width()-20)+'px'});
}
	


//heights 
//handle one column tables
if(tbl.find(".scrolltable-scroll td, .scrolltable-scroll th").length == 0)
{
	tbl.find(".scrolltable-scroll").remove();
	tbl.css({'width':'100%'});
	tbl.find(".scrolltable-fixed:first").css({'width':'100%'});
	
}


syncRowHeights(tbl);

if(hidden===1)
$(t).closest('.ui-accordion-content').hide();

$(t).remove();
});//end each table


 //fix up disclosure history
 $(".disclosureTabBody").removeClass("disclosureTabBody").addClass("card-wrapper card-2").css({'float':'left'});
 
 //fix disclosure search
 if(window.location.href.toLowerCase().indexOf("investors/investor_search_tools/pages/default.aspx")>-1)
 {
 $(".gmFilterBlock").addClass("card-wrapper card-2").css({"float":"left"});
 $(".gmFilterBlock").parent().attr('style', ' ');
 	$(".gmFilterBlock").find("p").css({"float":"left"});
 	
 	$(".filterBlock:last").css({"clear":"left","display":"block","width":"100%"}).find("input[name='buttonSubmit']").css('width','100%');
 	
 
 }


