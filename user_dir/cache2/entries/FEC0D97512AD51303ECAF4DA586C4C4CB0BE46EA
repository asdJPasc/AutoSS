$("div.gmFilterBlock")
	.closest("div").css({'display':'inline-block', 'padding-left' : '2px !important', 'padding-right':'2px !important'})
	.find(".buttonClear").css({"margin-left":"0px"});;

}

}//end if burger
 

}

function syncRowHeights(tbl){
//console.log('setting row height');
var max = 0;
tbl.find("table:not('.detailsTable') th").each(function(i,th){
	// console.log($(th).text());
		    max = Math.max($(th).height(), max); //getHiddenDimensions(true).height
	//$(tr).find("th,td").css({'height':max+"px"});
});//end each th
//console.log(max);
max = max + 2;
tbl.find("table:not('.detailsTable') th").css({'height': max+'px','min-height':max+'px'});
max = 0;
tbl.find(".scrolltable-scroll-inner table:not('.detailsTable') tr").each(function(tri,tr){
max=0;
$(tr).find('td').each(function(tdi,td){
max = Math.max($(td).height(), max);
});//end each td
//get left side height
var lH =  tbl.find(".scrolltable-fixed table  tr:eq("+tri+") td:first").height();
max = max > lH ? max : lH;
max = max + 2;
tbl.find(".scrolltable-fixed table tr:eq("+tri+") td:first").css({'height': max+'px','min-height':max+'px'});
$(tr).find("td").css({'height': max+'px','min-height':max+'px'});


});//end each table

}

function runLetterFilter(letter){
var str = updateQueryStringParam("letter",letter);
window.location.href=str;
}	 
	 

$(document).on('DOMNodeInserted', function (e) {
var t = $(e.target).prop('id'); 
if(t!=null && typeof t != 'undefined'){
if(t.indexOf('filterTable')>-1){
//console.log('fired' + t);
formatMobileTables(t);
}
}
});

if(window.location.href.toLowerCase().indexOf("profile.aspx")>-1 || window.location.href.toLowerCase().indexOf("subscribe.aspx")>-1){
//console.log('profile page');
$("#formcontainer").ready(function(){
if ($("#bm-nav-icon:visible").length > 0) {
var lpl;
var loginPanel = $("<div class='loginPanel'></div>").insertAfter("#formcontainer");
loginPanel.closest("td[id*='MSOZoneCell']").closest("table").wrap("<div class='logincard card-2'></div>");
var clone = $("#formcontainer");//.clone();
loginPanel.prop('id','formcontainer');
//$("#formcontainer").remove();
$("#formcontainer").hide();
clone.find(" > table > tbody > tr").find(">td").each(function(fei,fe){
if($(fe).hasClass('sub-label') && ($(fe).parent().prop('id')!=null && $(fe).parent().prop('id').indexOf('trOrg')===-1)){                 //, td.sub-entry, td:has(input.sub-button,input.btnCancel)
	lpl = $("<div class='loginPanelLabel'></div>").appendTo(loginPanel);
	$(fe).contents().appendTo(lpl);
}else if($(fe).find('table').length>0){
	lpl = $("<div class='loginPanelField'></div>").appendTo(loginPanel);
	if($(fe).find("table.subCbList").length>0 || $(fe).find("table[id*='rbMedia']").length>0){
		$(fe).find("table:first").appendTo(lpl);//.clone().
	}else{
		$(fe).find("table:first td:not(:has('.buttonSearch'))").contents().clone().appendTo(lpl);//.contents().clone().
		$(fe).find("table:first td:has('.buttonSearch')").contents().wrap("<div class='loginPanelField'></div>").appendTo(loginPanel);//.contents().clone()
	}
}else if($(fe).hasClass('sub-entry') || $(fe).find("input.sub-button,input.btnCancel").length>0){ 
	lpl = $("<div class='loginPanelField'></div>").appendTo(loginPanel);
	var ctrls = $(fe).contents().wrap("<div/>");//.contents().clone()

	if($(fe).parent().attr('id') != null && $(fe).parent().attr('id').indexOf('trOrg')>-1){
		var tb = $("<table class='orgbox'><tbody><tr id='" + $(fe).parent().attr('id') +"'><td><table><tbody><tr></tr><tr></tr></tbody></table></td></tr></tbody></table>").appendTo(lpl);
		var l = $(fe).closest("tr").find("td:eq(0)");//.clone();
		var f = $(fe).closest("tr").find("td:eq(1)");//.clone();
		
		l.appendTo(tb.find("table tbody tr:eq(0)"));
		f.appendTo(tb.find("table tbody tr:eq(1)"));
		//$(fe).closest("tr").remove();
	}
	else{
		ctrls.appendTo(lpl);
	}
}else if(($(fe).parent().prop('id')!=null && $(fe).parent().prop('id').indexOf('trOrg')===-1)){
lpl = $("<div class='loginPanelLabel'><div class='sub-instruct'></div></div>").appendTo(loginPanel);
$(fe).contents().appendTo(lpl.find(".sub-instruct"));
}

});

//$("#formcontainer").prop('id','oldformcontainer').remove();
//$("input[type='radio'][id*='_0']").each(function(i,e){$(e).attr('id',$(e).attr('id').replace('_0',''));})
var orgVal = $(".orgbox").closest("div").prev().find("input[id*='rbMediaUpdate']:checked").val();
if(orgVal==="1")
	$(".orgbox tr[id*='trOrg']").show();


loginPanel.show();

}
});
}

var updateQueryStringParam = function (key, value) {

    var baseUrl = [location.protocol, '//', location.host, location.pathname].join(''),
        urlQueryString = document.location.search,
        newParam = key + '=' + value,
        params = '?' + newParam;

    // If the "search" string exists, then build params from it
    if (urlQueryString) {
        var updateRegex = new RegExp('([\?&])' + key + '[^&]*');
        var removeRegex = new RegExp('([\?&])' + key + '=[^&;]+[&;]?');

        if( typeof value == 'undefined' || value == null || value == '' ) { // Remove param if value is empty
            params = urlQueryString.replace(removeRegex, "$1");
            params = params.replace( /[&;]$/, "" );

        } else if (urlQueryString.match(updateRegex) !== null) { // If param exists already, update it
            params = urlQueryString.replace(updateRegex, "$1" + newParam);

        } else { // Otherwise, add it to end of query string
            params = urlQueryString + '&' + newParam;
        }
    }

    // no parameter was set so we don't need the question mark
    params = params == '?' ? '' : params;
	return baseUrl + params;
    //window.history.replaceState({}, "", baseUrl + params);
};

//handle images that are content areas on mobile pages, allowing them to be viewed in full in a new window when clicked, and fixing sizing issues

$("div.boxedContent").ready(function(){
if ($("#bm-nav-icon:visible").length > 0) {
var bcW = $(window).width();
var matches = $(".boxedContent a[target='_blank'] img:not([src*='blank.gif']):not([class*='leadership-img']):not([src*='GinnieInBrief']):not([src*='ginnieinbrief']):not([src*='youtube.com'])").filter(function(i,im){return $(this).width() > bcW; });
matches.prop('width','100%')
	.prop('height','')
	.css({'width':'100%','height':'auto','margin':'auto', 'padding':'inherit'})
	.parent().append("<div style='cursor:pointer;'>Click to view full size image</div>");

 

$(".boxedContent img[alt]:not([src*='blank.gif']):not([class*='leadership-img']):not([src*='GinnieInBrief']):not([src*='ginnieinbrief']):not([src*='youtube.com'])").filter(function(i,im){
if($(this).width() > bcW || $(this).parents("a").length===0){return true;} }).each(function(i,im){
	$(im).prop('width','100%')
	.prop('height','')
	.css({'width':'100%','height':'auto','margin':'auto','padding':'inherit'});
	if(!$(im).parent().is("a")){
		$(im).wrap("<a title='Click to view full image' href='"+$(this).attr('src')+"' target='_blank'></a>");
		$(im).parent().append("<div style='cursor:pointer;'>Click to view full size image</div>");
		}
	});
	}
});
var scrollHeight = 0;
$("#s4-workspace").ready(function(){
setInterval(function(){
var cScrollHeight = $("#s4-workspace").height();
if(cScrollHeight != scrollHeight){
scrollHeight = cScrollHeight;
$("#s4-workspace").attr('style',$("#s4-workspace").attr('style')+'-webkit-overflow-scrolling: touch;-webkit-perspective: 0;');
}
},800);
});


$("div.boxedContent").ready(function () {
    //--RH 8/17/2018  below code check if device is mobile phone or tablet
    if ($("#bm-nav-icon:visible").length > 0) {
        var windWidth = $(window).width();
        var mobileWidth = 450;

        //--RH 8/17/2018  below code check if .imgPlusName class exist in the page
        if ($('.imgPlusName')[0]) {
            var boxWidth = $('.imgPlusName').width() / $('.imgPlusName').parent().width() * 100;
            //--RH 8/17/2018  below code check if device is mobile phone
            if (windWidth <= mobileWidth) {
                //--RH 8/17/2018  check if the page is Modernization
                if (boxWidth > 25 && boxWidth < 45) {
                    $(".imgPlusName").attr('style', 'width: 30.5% !important; height: 140px');
                }
                //--RH 8/17/2018  affect pages that have 4 boxes like in MBS Guide
                else {
                    $(".imgPlusName").attr('style', 'width: 45.5% !important');
                }
            }
            //--RH 8/23/2018  when device is tablet
            else {
                $(".imgPlusName").attr('style', 'margin-top: 10px; margin-bottom: 10px');
            }
        }

        //--RH 8/17/2018  below code check if .headerDiv class exist in the page
        if ($('.headerDiv')[0]) {
            var headerDivHeight = $('.headerDiv').height();
            //--RH 8/17/2018 .headerDev class has height of 215 in MBâ€‹SProspectuses and Budget & Performance pages
            if (headerDivHeight == 215) {
                $(".headerDiv").attr('style', 'height: 230px !important');
            }
            //--RH 8/17/2018 .headerDev class has height of 275 in Systems & Applications page
            else if (headerDivHeight == 275) {
                $(".headerDiv").attr('style', 'height: 340px !important');
            }
        }


        //--RH 8/23/2018  below code check if .filter-tableCell1 class exist in the page
        if ($('.filter-tableCell1')[0]) {
            //--RH 8/23/2018  if the device is mobile phone
            if (windWidth <= mobileWidth) {
                $(".filter-tableCell1").attr('style', 'width: 45.5% !important');
                $(".wrapperDiv ").attr('style', 'max-width: 80% !important');
            }
            //--RH 8/23/2018  affect pages that have 4 boxes like in MBS Guide.  This will happen in Tablet device
            else {
                $(".filter-tableCell1").attr('style', 'width: 24.5% !important');
                $(".wrapperDiv ").attr('style', 'width: 75% !important');
            }
        }
    }
   
});

