hidden=1;
	$(t).closest('.ui-accordion-content').show();
}
var newSTbl = $("<div class='scrolltable' id='scrolltable_"+i+"'><div class='scrolltable-fixed'><table cellspacing='0'><thead></thead><tbody></tbody><tfoot></tfoot></table></div><div class='scrolltable-scroll'><div class='scrolltable-scroll-inner'><table  cellspacing='0'><thead></thead><tbody></tbody><tfoot></tfoot></table></div></div></div>").insertAfter(t);//" + $(t).attr('class')+"
//newTbl.wrap("<div class='zui-wrapper'><div class='zui-scroller'></div></div>");

$(t).find("#hiddenresults").closest("tr").remove();
