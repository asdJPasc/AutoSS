$(".disclosureFilterTable, #catfilter, #keywordsearchtbl, div.gmFilterBlock table").each(function(dti,dt){
//handle page size
var pgsz = $("th.pagesize").contents();
if(pgsz.length > 0){
$("th.pagesize").remove();
}
$(dt).hide();
var ftc = $("<div class='filter-table-cont' id='filter-table-cont-"+dti+"'></div>").insertBefore(dt);
var block;
var ctrl;
$(dt).find("tr td:has('input,select')").each(function(i,r){
ctrl = $(r).contents().not('.inputSearchIcon');
if(ctrl!=null){
var isSB = (ctrl.length > 0 && ctrl[0].id ==="disclosureSearchBox");
block = $("<div class='filterBlock' style=''><div class='filterBlockLabel'>"+(ctrl.length > 0 && ctrl[0].id ==="disclosureSearchBox" ? "Search:" : $(r).closest("tr").prev().find("th").eq(i).text())+"</div><div class='filterBlockControl'></div></div>").appendTo(ftc);
if(!(i % 2 === 0) && !(isSB) && block.prev().width() < 250)
	block.css({'float':'right'});
else if(isSB){
	block.css({'clear':'left','width':'100%'});
	if(block.prev().css('float')==='left'){block.prev().css('width','100%');}
}else if(block.prev().width() > 250)
{
block.css({'width':'100%'});
}
else
	block.css({'clear':'left'});
	
block.find("div.filterBlockControl").append(ctrl);
}

});
if(pgsz.length > 0)
{
$("<div class='filterBlock' style='float:left;width:100%;'><div class='filterBlockLabel'></div><div class='filterBlockControl'></div></div>").appendTo(ftc);
ftc.find(".filterBlock:last .filterBlockControl").append(pgsz);
}

});



