	block.css({'float':'right'});
else if(isSB){
	block.css({'clear':'left','width':'100%'});
	if(block.prev().css('float')==='left'){block.prev().css('width','100%');}
}else if(block.prev().width() > 250)
{
block.css({'width':'100%'});
}
else
	block.css({'clear':'left'});
	
block.find("div.filterBlockControl").append(ctrl);
}

});
if(pgsz.length > 0)
{
