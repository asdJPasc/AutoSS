return true;
}else{
if($(place).prev().hasClass("mobileTableHeader"))
	$(place).prev().append(" > " + $(tr).find("th:first,td:first").not(":contains('There are no')").text());
else
	$("<span class='mobileTableHeader'>"+$(tr).find("th:first,td:first").not(":contains('There are no')").text()+"</span>").insertBefore(place);
	
return true;
}

}




if($(tr).find("th").length > 0 || $(tr).hasClass('tableSubHeader')){ //header row
	//insert first row / cell into left col
	$(tr).prop('class','').prop('style','');//clear styles
	$(tr).find("tr,th,td").prop('class','').prop('style','');//clear styles

	var newLftTr = $("<tr class='scrolltable-header'></tr>").appendTo(newSTbl.find(".scrolltable-fixed > table > thead"));
	$(tr).find("td:eq(0),th:eq(0)").append("&nbsp;").appendTo(newLftTr);
	 
	
	var newRtTr = $("<tr class='scrolltable-header'></tr>").appendTo(newSTbl.find(".scrolltable-scroll-inner > table > thead"));
	$(tr).find("td,th").appendTo(newRtTr);
 
}
else //regular rows
{
	$(tr).prop('class','').prop('style','');//clear styles
	$(tr).find("tr,th,td").prop('class','').prop('style','');//clear styles

var newLftTr = $("<tr></tr>").appendTo(newSTbl.find(".scrolltable-fixed > table > tbody"));
	if(rS===0){
		var oldTD = $(tr).find("td:eq(0)").clone();
		var rowSpan = oldTD.attr('rowspan');
		if(typeof rowSpan != 'undefined'){
		rS = parseInt(rowSpan);
		
		
		oldTD.removeAttr('rowspan');
		}
		oldTD.appendTo(newLftTr);
			if(rS > 0){
			for(rsi=1;rsi<rS;rsi++){
			var bg = newSTbl.find(".scrolltable-fixed > table > tbody > tr:last > td:first").css('background-color');
			$("<tr><td style='background-color:"+bg+"'>&nbsp;</td></tr>").appendTo(newSTbl.find(".scrolltable-fixed > table > tbody"));	
			}
			
			rS= rS -1;
			}
		}else{
		rS = rS - 1;
	}
	var newRtTr = $("<tr></tr>").appendTo(newSTbl.find(".scrolltable-scroll-inner > table > tbody"));
	if(rS ===0)
		$(tr).find("td:not(:first)").appendTo(newRtTr);
	else
		$(tr).find("td:not([rowspan])").appendTo(newRtTr);

}
});//end each row
//newSTbl.find("tr,th,td").prop('class','').prop('style','');//clear styles
